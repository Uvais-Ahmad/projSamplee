<!-- this is the partial which set using include() function inside EJS tags -->
<h4>Page Of Header</h4>

<ul>
<!-- locals is a variable of req and it store the another onbject name user -->
    <% if(locals.user) { %>
        
        <li>
            <h2><a href="/users/profile" ><%= user.name %></a> </h2>
        </li>
        <li >
            <a href="/users/sign-out">Sign Out</a>
        </li>
        
    <% } else{ %> 
        <li>
            <a href="/users/sign-in" >Sign In</a>
        </li>

        <li>
            <a href="/users/sign-up" >Sign Up</a>
        </li>
    <% } %> 
</ul>